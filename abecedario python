abecedario = ['a', 'b', 'c', 'd', 'e', 'f', 'g', 'h', 'i', 'j', 'k', 'l','m',
              'n','Ã±', 'o', 'p', 'q', 'r', 's', 't', 'u', 'v', 'w', 'x', 'y', 'z']

print (len(abecedario))

for i in range(len(abecedario), 1, -1):
    if (i+1) % 3 == 0:
        abecedario.remove(abecedario[i])
        
print(abecedario)


